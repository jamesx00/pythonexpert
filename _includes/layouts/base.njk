    <body x-data="{fullScreen: $persist(false).as('fullScreen')}">
        {% if not hide_navbar %}
            {% include 'navigation.njk' %}
        {% endif %}
        <main id="skip">
            {{ content | safe }}
        </main>
        {% include 'footer.njk' %}
        <!-- This page `{{ page.url | htmlBaseUrl }}` was built on {% currentBuildDate %} -->
    </body>
</html>